class itemPrice 
{
	public static void main(String[] args) 
	{
		
		double P=1298; //original price
		System.out.println("Original Price:"+P);
		double D=25; //Discount Percentage
		double T=18; //Sales Tax Percentage
		double finalPrice=(P * (1 - (D/100)) * (1 + (T/100)));
		System.out.println("Final Price Is:"+finalPrice);
	}
}