import java.util.*;
class SumOfArray
{
	static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) 
	{
		System.out.print("Enter Your N'th Number:");
		int N=sc.nextInt();
		int A[]=new int[N];
		//1.Sum of Array Elements
		int Sum=0;
		for (int i=0;i<N-1;i++)
		{
			Sum+=A[i];
		}
		System.out.println("Hello World!");
	}
}
